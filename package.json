{
  "name": "guide-app",
  "version": "1.0.0",
  "description": "guide-app",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "compile:sass": "node-sass sass/main.scss css/style.css",
    "devserver": "live-server",
    "start": "npm-run-all --paraller devserver watch:sass",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "prefix:css": "postcss --use-autoprefixer -b 'last 10 versions' style.concat.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:sass concat:css prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ElzbietaSzulc/guide-app.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ElzbietaSzulc/guide-app/issues"
  },
  "homepage": "https://github.com/ElzbietaSzulc/guide-app#readme",
  "devDependencies": {
    "autoprefixer": "^9.7.3",
    "node-sass": "^4.13.0",
    "postcss-cli": "^7.1.0"
  },
  "dependencies": {
    "jquery": "^3.4.1",
    "live-server": "^1.2.1"
  }
}
